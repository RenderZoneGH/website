        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "app/routes/__pycache__": true,
        "app/__pycache__": true,
        "app/util/__pycache__": true,
        "app/routes/api/__pycache__": true
